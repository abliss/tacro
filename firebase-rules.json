{
    "rules": {
        ".read": true,
        "users": {
            "$user_id": {
                ".write": "$user_id === auth.uid"
            },"checked": {
                ".write": false
            },
        },
        "incoming": {
            "fact":{
                ".write": "!data.exists() && newData.exists()"
            },
            "state":{
                ".write": "!data.exists() && newData.exists()"
            },
            "log":{
                ".write": "!data.exists() && newData.exists()"
            },
        }
    }
}