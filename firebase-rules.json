{
    "rules": {
        "tacro": {
            ".read": true,
            "users": {
                "$user_id": {
                    ".write": "$user_id === auth.uid"
                }
            },
            "incoming": {
                "fact":{
                    "$pushed": {
                        ".write": "!data.exists()"
                    }
                },
                "state":{
                    "$pushed": {
                        ".write": "!data.exists()"
                    }
                },
                "log":{
                    "$pushed": {
                        ".write": "!data.exists()"
                    }
                },
            }
        }
    }
}